{
  "name": "monorepo",
  "version": "1.1.1",
  "private": true,
  "packageManager": "pnpm@9.12.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --scope=web...",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,json}\"",
    "test": "turbo run test",
    "cypress:open": "turbo run cypress:open"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "^3.3.3",
    "turbo": "^2.2.3"
  },
  "engines": {
    "node": "^20.17.0"
  },
  "dependencies": {
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14"
  }
}
