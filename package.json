{
  "name": "monorepo",
  "version": "1.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --scope=web...",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,json}\"",
    "test": "turbo run test",
    "cypress:open": "turbo run cypress:open"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "^3.0.0",
    "turbo": "^1.9.9"
  },
  "engines": {
    "node": "16"
  },
  "dependencies": {
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2"
  },
  "packageManager": "pnpm@8.6.8"
}
