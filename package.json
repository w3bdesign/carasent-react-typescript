{
  "name": "monorepo",
  "version": "1.1.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --scope=web...",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,json}\"",
    "test": "turbo run test",
    "cypress:open": "turbo run cypress:open"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "^3.0.3",
    "turbo": "^1.10.14"
  },
  "engines": {
    "node": "20"
  },
  "dependencies": {
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3"
  },
  "packageManager": "pnpm@8.14.1"
}
